
<div>
<Bonfire.UI.Social.FeedLive 
    id={@feed_id}
    feed={@feed}
    page_info={@page_info}
    showing_within={@showing_within}
    feed_update_mode={@feed_update_mode}
    hide_load_more={true}
  />
  <div :if={length(@feed) > 0 } class="p-2 border-t border-base-300">
    <LivePatch to={path(:notifications)} class="w-full normal-case btn btn-wide btn-sm">{l "More notifications"}</LivePatch>
  </div>
</div>